@model CallStatistic.Models.Calls

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>Calls</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="dateOf" class="control-label"></label>
                <input asp-for="dateOf" class="form-control" />
                <span asp-validation-for="dateOf" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="fromPhone" class="control-label"></label>
                <input asp-for="fromPhone" class="form-control" />
                <span asp-validation-for="fromPhone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="toPhone" class="control-label"></label>
                <input asp-for="toPhone" class="form-control" />
                <span asp-validation-for="toPhone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="duration" class="control-label"></label>
                <input asp-for="duration" class="form-control" />
                <span asp-validation-for="duration" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
